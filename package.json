{
  "name": "ela-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-debug": "next build --debug",
    "build": "next build",
    "check-types": "tsc --noEmit",
    "create-wp-gql-ts": "npm run reminder && npm run create-wp-gql && npm run create-wp-ts",
    "create-wp-gql": "node gql-ts/get-wp-gql-schema.mjs",
    "create-wp-ts": "graphql-codegen --config gql-ts/codegen.yml && npm run lint-wp-ts",
    "dev": "next dev --hostname lampel-3.local",
    "dev-alt": "next dev --hostname lampel-3.local -p 3001",
    "lint-wp-ts": "eslint --fix gql-ts/wp-graphql.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "prepare": "husky install",
    "postbuild": "next-sitemap",
    "reminder": "echo 'REMEMBER TO DISABLE INTROSPECTION!'",
    "start": "next start"
  },
  "dependencies": {
    "@reach/dialog": "^0.15.0",
    "@reach/menu-button": "^0.15.1",
    "@reach/tabs": "^0.15.0",
    "@reach/visually-hidden": "^0.15.0",
    "@types/gtag.js": "^0.0.7",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-map-gl": "^5.2.9",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-map-gl": "^6.1.15",
    "react-query": "3.15.2",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "next-sitemap": "^1.6.140",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1"
  },
  "lint-staged": {
    "*/**/*.{js,ts,tsx}": "eslint  --quiet --cache --fix"
  },
  "peerDependencies": {
    "mapbox-gl": "^2.2.0"
  }
}
